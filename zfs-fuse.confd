# zfs-fuse configuraton file

# array with dataset (pools) to import
# use "-a" to import everything

ZFS_IMPORT=("") # ("-a")

# array with datasets to mount
# use "-a" to mount everything
# this has to be a subset of ZFS_IMPORT or mounting will fail

ZFS_MOUNT=("") # ("-a")

# how long to wait for zfs-fuse daemon to come up
ZFS_DELAY=1
